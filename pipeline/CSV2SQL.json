{
	"name": "CSV2SQL",
	"properties": {
		"description": "Transer CSV data to SQL Azure",
		"activities": [
			{
				"name": "CSV2SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "John",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "firstname",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Doe",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "lastname",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "USA",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "country",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "CsvUser",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "UserDatabase",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}